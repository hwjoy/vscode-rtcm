{
  "name": "vscode-rtcm",
  "displayName": "RTCM",
  "description": "RTCM parse for VS Code",
  "version": "0.2.2",
  "publisher": "gnssimu",
  "license": "MIT",
  "homepage": "https://github.com/hwjoy/vscode-rtcm",
  "bugs": {
    "url": "https://github.com/hwjoy/vscode-rtcm/issues",
    "email": "gnssimu@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hwjoy/vscode-rtcm.git"
  },
  "qna": "marketplace",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "RTCM", "GNSS", "BDS", "RNIEX"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-rtcm.rtcm",
        "title": "RTCM Parse"
      },
      {
        "command": "vscode-rtcm.changeDate",
        "title": "RTCM Change Date"
      }
    ],
    "languages": [
      {
        "id": "rtcm",
        "aliases": [
          "RTCM",
          "rtcm"
        ],
        "extensions": [
          ".rtcm",
          ".track",
          ".gps"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rtcm",
        "scopeName": "source.rtcm",
        "path": "./syntaxes/rtcm.tmLanguage.json"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceExtname == .rtcm || resourceExtname == .track || resourceExtname == .gps",
          "command": "vscode-rtcm.rtcm",
          "group": "navigation"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "rtcm.rtcmPreview",
        "displayName": "RTCM File",
        "selector": [
          {
            "filenamePattern": "*.rtcm"
          },
          {
            "filenamePattern": "*.track"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "vscode:prepublish": "npm run package",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
