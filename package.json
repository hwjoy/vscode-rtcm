{
  "name": "vscode-rtcm",
  "displayName": "RTCM",
  "description": "RTCM parse for VS Code",
  "version": "0.2.0",
  "publisher": "gnssimu",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "vscode-rtcm.rtcm",
      "title": "RTCM Parse"
    }, {
      "command": "vscode-rtcm.changeDate",
      "title": "RTCM Change Date"
    }],
    "languages": [{
      "id": "rtcm",
      "aliases": ["RTCM", "rtcm"],
      "extensions": [".rtcm", ".track", ".gps"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rtcm",
      "scopeName": "source.rtcm",
      "path": "./src/syntaxes/rtcm.tmLanguage.json"
    }],
    "menus": {
      "editor/context": [{
        "when": "resourceExtname == .rtcm || resourceExtname == .track || resourceExtname == .gps",
        "command": "vscode-rtcm.rtcm",
        "group": "navigation"
      }]
    },
    "customEditors": [{
      "viewType": "rtcm.rtcmPreview",
      "displayName": "RTCM File",
      "selector": [{
        "filenamePattern": "*.rtcm"
      }, {
        "filenamePattern": "*.track"
      }],
      "priority": "default"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
